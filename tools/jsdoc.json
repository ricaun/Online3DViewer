{
	"source": {
		"include": ["source"]
	},
    "opts": {
        "template": "node_modules/ink-docstrap/template/",
        "encoding": "utf8",
        "destination": "build/docs/",
        "recurse": true,
        "verbose": false
    },
    "plugins": [
        "plugins/markdown"
    ],
	"docdash" : {
		"menu": {
            "GitHub": {
                "href":"https://github.com/kovacsv/Online3DViewer",
                "target":"_blank"
            }
        }
	},
    "templates": {
        "includeDate": false,
        "theme": "flatly",
        "navType" : "inline",
        "sort" : false
    }
}
